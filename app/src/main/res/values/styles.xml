<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="EditTextWhite">
        <item name="android:textColorHint">#787878</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="backgroundTint">@color/white</item>
    </style>
    <style name="Title">
        <item name="android:fontFamily">@font/tenor_sans</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">14sp</item>
        <item name="fontWeight">400</item>
        <item name="lineHeight">20dp</item>
    </style>
    <style name="SubTitle">
        <item name="android:fontFamily">@font/tenor_sans</item>
        <item name="android:textColor">@color/grayScale</item>
        <item name="android:textSize">12sp</item>
        <item name="fontWeight">400</item>
        <item name="lineHeight">18dp</item>
    </style>
</resources>